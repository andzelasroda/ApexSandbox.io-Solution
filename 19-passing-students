public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    if(passedExam=true && passedAssignments=true && passedProject = false)){
        return true;
    }
    if(passedExam=false && passedAssignments=true && passedProject = true){
        return true;
    }
    if(passedExam=true && passedAssignments=false && passedProject = false){
        return true;
    }
    else{
        return false;
    }
}